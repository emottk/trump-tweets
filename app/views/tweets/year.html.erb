                <h5 class="font-accident-one-bold title uppercase hovercolor">That's one tweet every 3.5 hours</h5>
             </div>
             <div class="col-md-12 dividewhite2"></div>
             <div class="col-md-12 wow fadeInDown" data-wow-delay="0.4s" data-wow-offset="10"><div id='howdy' class="img-responsive"></div></div>
          </div>
       </div>
    </section>
    <!-- /Services Block -->


