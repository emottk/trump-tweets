  <%#= render 'learn_more' %>


  <div id="content" class="bg-color00">
    <!-- Counts Block -->
    <section id="counts" class="counts e-block-ins e-bg-light-texture pt-section" data-stellar-background-ratio="0.5" data-name="Counts">
       <div class="container">
          <div class="count-container row">
             <div class="col-md-12 count">
                <div class="count-icon">
                   <!-- <i class="flaticon-photo246"></i> -->
                </div>
                <span class=".integers digit font-accident-two-bold">2500</span>
                <div class="count-text font-accident-one-bold">Tweets</div>
                <div class="divider-dynamic"></div>
             </div>
          </div>
       </div>
    </section>
    <!-- /Counts Block -->


